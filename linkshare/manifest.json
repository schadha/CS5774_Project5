{
  "manifest_version": 2,

  "name": "Track Uploader",
  "description": "This extension lets users of CollabTunes add an mp3 track with one click.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "link_icon.png",
    "default_popup": "popup.html"
  },
    
  "background": {
      "scripts": ["background.js"]
  },
    
  "permissions": [
    "http://localhost/CS5774/Project5/",
    "storage",
      "tabs"
  ]
}
